<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:TransformacionesGrafics.Graficos"
             x:Class="TransformacionesGrafics.Views.PaginaPrincipal"
             Title="PaginaPrincipal">
    <ContentPage.Resources>
        <local:EstrellaTranslate x:Key="Translate"/>
    </ContentPage.Resources>
    <VerticalStackLayout>
        <GraphicsView Drawable="{StaticResource Translate}"
                      WidthRequest="500"
                      HeightRequest="500"
                      HorizontalOptions="Center"/>
    </VerticalStackLayout>
</ContentPage>